<!-- DASHBOARD HEADER -->
<div class="row border-bottom white-bg dashboard-header">
  <div class="col-lg-4">
    <h2><i class="fa fa-desktop"></i> &nbsp;<%= current_user.website_url.gsub('www.','') %></h2>
    <small><i class="fa fa-times"></i> Non verificato. Verifica ora il tuo business.</small>
    <hr>
    <a href="/demoseo" class="btn btn-primary btn-xs">Cancella sito e ripeti analisi</a> &nbsp;
  </div>
  <div class="col-lg-4">
    <ul class="stat-list m-t">
    <li>
      <small><i class="fa fa-check"></i> Test passati</small>
      <span class="label label-primary pull-right">10</span>
      <div class="progress progress-mini">
        <div style="width: 50%;" class="progress-bar"></div>
      </div>
    </li>
    <li>
      <small><i class="fa fa-warning"></i> Test parzialmente passati</small>
      <span class="label label-warning pull-right">5</span>
      <div class="progress progress-mini">
        <div style="width: 25%;" class="progress-bar progress-bar-warning"></div>
      </div>
    </li>
    <li>
      <small><i class="fa fa-times"></i> Test non passati</small>
      <span class="label label-danger pull-right">4</span>
      <div class="progress progress-mini">
        <div style="width: 20%;" class="progress-bar progress-bar-danger"></div>
      </div>
    </li>
  </ul>
  </div>
  <div class="col-lg-offset-1 col-lg-3">
    <div class="widget navy-bg no-padding">
      <div class="p-m">
        <h1 class="m-xs"><%= @report.points %></h1>

        <h3 class="font-bold no-margins">
            Punteggio Footprint SEO
        </h3>
        <small>Ti consigliamo di incrementare fino a 73.</small>
      </div>
    </div>
  </div>
</div>
<!-- END DASHBOARD HEADER -->

